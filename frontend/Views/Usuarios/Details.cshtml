@model frontend.Models.Usuarios

@{
    ViewData["Title"] = "Detalles del Usuario";
}

<h2>Detalles del Usuario</h2>

<div>
    <h4>Usuario</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Model.Id
        </dd>
        <dt class="col-sm-2">
            Gmail
        </dt>
        <dd class="col-sm-10">
            @Model.Gmail
        </dd>
        <dt class="col-sm-2">
            Contraseña
        </dt>
        <dd class="col-sm-10">
            @Model.Contrasena
        </dd>
    </dl>
</div>
<p>
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
</p>
